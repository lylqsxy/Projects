                                        <label for="dndo" class="toggle"><span class="toggle__handler"></span></label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        @*  Author Aakash
                            to capture socialmedia details
                        *@
                        <div>
                          
                            @using (Html.BeginForm("CreateSocialMedia", "Manage"))
                            {
                                @Html.AntiForgeryToken()
                                <p><b> Register your social Media</b></p>
                             

                                <ul style="list-style-type:none;">
                                    @foreach (AuthenticationDescription p in loginProviders)
                                    {
                                        <li>
                                            @if (p.AuthenticationType == "Google")
                                            {
                                               
                                                <button type="submit"  ng-if="socialMediaGo.length != null"  @*ng-disabled="isDisabled" ng-click="DisableButton()"*@ class="btn btn-danger googleplus"  id="@p.AuthenticationType" name="provider" value="@p.AuthenticationType" title="Log in using your @p.Caption account"><i class="fa fa-google-plus"></i></button>
                                                <p ng-show="socialMediaGo.length !== 0">Google Registered</p>
                                            <br />
                                            <br />
                                                    
                                            }

                                            else if (p.AuthenticationType == "Facebook")
                                            {
                                                <p ng-show="socialMediaFa.length !== 0">Facebook Regestered</p>
                                                <button type="submit"  ng-if="socialMediaFa.length != null"  @*ng-disabled="isDisabled" ng-click="DisableButton()"*@ class="btn  btn-primary facebook" id="@p.AuthenticationType" name="provider" value="@p.AuthenticationType" title="Log in using your @p.Caption account"><i class="fa fa-facebook"></i></button>
                                                <br />
                                            }
                                        </li>
                                    }
                                </ul>
                            }

                        </div>

                    </div>
                </div>
            </div>
        </div>
